{
  "id": "/siegeclients",
  "groups": [
    {
      "id": "/siegeclients/siegegroup",
      "apps": [
        {
          "id": "siege",
          "args": [
            "-d@delay@",
            "--time=@time@",
            "-c@nclients@",
            "http://@endpoint@/"
          ],
          "cpus": 0.5,
          "mem": 128.0,
          "instances": @ninstances@,
          "container": {
            "type": "DOCKER",
            "volumes": [],
            "docker": {
              "image": "yokogawa/siege",
              "network": "HOST",
              "privileged": false,
              "parameters": [],
              "forcePullImage": false
            }
          }
        }
      ]
    }
  ]
}